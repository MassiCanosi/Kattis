   "source": [
    "## Average Character"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "usr_input = input()\n",
    "ascii_values = [ord(char) for char in usr_input]\n",
    "accum = 0\n",
    "\n",
    "for i in ascii_values:\n",
    "    accum += i\n",
    "\n",
    "mean = accum//len(ascii_values)\n",
    "\n",
    "otp = chr(mean)\n",
    "print(otp)"
   ]
  }
